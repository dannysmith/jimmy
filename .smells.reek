---
exclude_paths:
  - node_modules
  - db/migrate
IrresponsibleModule:
  exclude:
  - ApplicationCable::Channel
  - ApplicationCable::Connection
  - ApplicationController
  - ApplicationHelper
  - ApplicationJob
  - ApplicationMailer
  - ApplicationRecord
  - Jimmy::Application
UtilityFunction:
  exclude:
  - Jimmy::Error
  - Jimmy::ExampleError
